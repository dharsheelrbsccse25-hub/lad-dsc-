#include<stdio.h>
#include<string.h>
#include<stdlib.h>
int main(){
     char a[50],b[50],c[50];
     int count,i;
    printf("Enter a string");
    scanf("%s",&a);
    for(i=0;i<strlen(a);i++)
    {
        c=tolower(a[i]);
        if(c='a'||c='e'||c='i'||c='o'||c'u')
        {
            count++;
        }
    printf("number of vowels is ",count)
    }
    return 0;
}
